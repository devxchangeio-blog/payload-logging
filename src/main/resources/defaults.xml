<included>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} -
                %msg%n
            </pattern>
        </encoder>
    </appender>
    <logger name="io.oneclicklabs.logging" level="INFO"/>
</included>